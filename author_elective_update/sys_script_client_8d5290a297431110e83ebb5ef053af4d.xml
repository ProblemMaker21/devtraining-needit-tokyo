        <description>Use the GetEmail Address Script Include to retrieve the Requested for's email address from the database when the Requested for value changes.&#13;
</description>
        <field>u_requested_for_email</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>GetEmailAdress&gt;populateEmailField</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {

  // Modified the if to return if the newValue == oldValue to avoid
  // unecessary trips to the server
  if (isLoading || newValue === '' || newValue == oldValue) {
    return;
var get = new GlideAjax('GetEmailAdress');
get.addParam('sysparm_name','getEmail');
 get.addParam('sysparm_userID');
g_form.getValue('u_requested_for');
  get.getXML(populateEmailField);
 function populateEmailField(response){
 var getit=
 response.responseXML.documentElement.getAttribute('answear');
 g_form.clearValue('u_requested_for');
 g_form.setValue('u_requested_for', getit);
 }
  }
}]]></script>
